<idea-plugin>
  <id>code naming</id>
  <name>code naming</name>
  <version>1.1</version>
  <description>
    Cloud programmer conditioner to help you solve naming problems, class naming, method naming
  </description>
  <change-notes>
    1.0：不知道什么时候发了第一版
    1.1：不知道什么时候会发第二版
    。。。。。。
  </change-notes>

  <idea-version since-build="171.0"/>

  <vendor url="http://www.bigGod.github,io" email="1253641040@qq.com" />

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>

  <actions>
    <!-- Add your actions here -->
    <action id="newClass" class="action.CloudNewClassAction" text="code naming" description="云端类创建">
      <add-to-group group-id="NewGroup" anchor="first"/>
    </action>


    <action id="CloudNewMethodAction" class="action.CloudNewMethodAction" text="code naming"
            description="CloudNewMethodAction">
      <add-to-group group-id="GenerateGroup" anchor="first"/>
    </action>
  </actions>

</idea-plugin>